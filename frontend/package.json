{
  "name": "video-crawler",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "npm run prepare-monaco && vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build && npm run copy-dist && node scripts/copy-monaco.js",
    "copy-dist": "node scripts/copy-dist.js",
    "preview": "vite preview",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "prepare-monaco": "node scripts/copy-monaco.js"
  },
  "dependencies": {
    "@guolao/vue-monaco-editor": "^1.5.5",
    "@types/crypto-js": "^4.2.2",
    "ant-design-vue": "^4.2.6",
    "crypto-js": "^4.2.0",
    "diff": "^8.0.2",
    "hls.js": "1.6.10",
    "monaco-editor": "^0.52.2",
    "pinia": "^3.0.3",
    "plyr": "3.7.8",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "terser": "5.43.1",
    "typescript": "~5.8.0",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vue-tsc": "^3.0.4"
  }
}
